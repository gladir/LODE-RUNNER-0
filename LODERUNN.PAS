Var
 Score:LongInt;
 Life,Level,Gold:Byte;
 XMan,YMan,XManRun,YManRun,WaitYManRun:Integer;
 CurrEnnemi:Byte;
 XEnnemi,YEnnemi:Array[0..10]of Byte;
 CurrTab:Array[0..15,0..27] of Char;

Function DuplicateString(C:Char;Num:Integer):String;
Var
 I:Byte;
 S:String;
Begin
 S:='';
 For I:=1 to Num do S:=S+C;
 DuplicateString:=S;
End;

Procedure LoadCurrLevel;
Var
 I,J:Byte;
Begin
 For J:=0 to 15 do For I:=0 to 27 do Begin
  CurrTab[J,I]:=Tab[Level,J][I+1];
 End;
 FillChar(XEnnemi,SizeOf(XEnnemi),0);
 FillChar(YEnnemi,SizeOf(YEnnemi),0);
End;

Procedure InitGame;Begin
 TextMode(CO40);
 {$IFNDEF FPC}
  LoadFont(224,0,16,7,@Font224);
 {$ENDIF}
 TextBackground(0);
 TextColor(7);
 ClrScr;
 CursorOff;
 Life:=3;
 Score:=0;
 Level:=1;
 XManRun:=0;
 YManRun:=0;
 WaitYManRun:=0;
 LoadCurrLevel;
End;

Procedure PutElem(X,Y:Byte);Begin
 Case CurrTab[Y,X] of
  '0':Begin
   CurrTab[Y,X]:=' ';
   XEnnemi[CurrEnnemi]:=X;
   YEnnemi[CurrEnnemi]:=Y;
   Inc(CurrEnnemi);
   TextColor(White);
   Write({$IFDEF FPC}'0'{$ELSE}Chr(228){$ENDIF});
   TextColor(7);
  End;
  '-':Begin
   TextBackground(0);
   TextColor(White);
   Write({$IFDEF FPC}'-'{$ELSE}Chr(230){$ENDIF});
   TextBackground(0);
   TextColor(7);
  End;
  '&':Begin
   CurrTab[Y,X]:=' ';
   XMan:=X;
   YMan:=Y;
   TextColor(Cyan);
   Write({$IFDEF FPC}'&'{$ELSE}Chr(228){$ENDIF});
   TextColor(7);
  End;
  '$':Begin
   TextColor(Yellow);
   Write({$IFDEF FPC}'$'{$ELSE}Chr(227){$ENDIF});
   TextColor(7);
  End;
  '#':Begin
   TextBackground(6);
   TextColor(Red);
   Write({$IFDEF FPC}'#'{$ELSE}Chr(224){$ENDIF});
   TextBackground(0);
   TextColor(7);
  End;
  '@':Begin
   TextBackground(6);
   TextColor(Red);
   Write({$IFDEF FPC}'@'{$ELSE}Chr(229){$ENDIF});
   TextBackground(0);
   TextColor(7);
  End;
  'H':Begin
   TextBackground(0);
   TextColor(White);
   Write({$IFDEF FPC}'H'{$ELSE}Chr(225){$ENDIF});
   TextBackground(0);
   TextColor(7);
  End;
  'S':Begin
   TextBackground(0);
   TextColor(White);
   Write({$IFDEF FPC}'S'{$ELSE}Chr(226){$ENDIF});
   TextBackground(0);
   TextColor(7);
  End;
  Else Write(CurrTab[Y,X]);
 End;
End;

Procedure ShowScore;Begin
 TextColor(15);
 TextBackground(0);
 GotoXY(1,18);
 Write('Pointage : ',Score);
End;

Procedure ShowLife;Begin
 TextColor(15);
 TextBackground(0);
 GotoXY(20,18);
 Write('Vie : ',Life);
End;

Procedure ShowLevel;
Var
 I,J:Byte;
Begin
 GotoXY(1,1);
 CurrEnnemi:=0;
 Gold:=0;
 TextBackground(6);
 TextColor(Red);
 WriteLn({$IFDEF FPC}' ':30{$ELSE}DuplicateString(Chr(177),30){$ENDIF});
 For J:=0 to 15 do Begin
  TextBackground(6);
  TextColor(Red);
  Write({$IFDEF FPC}' '{$ELSE}Chr(177){$ENDIF});
  TextBackground(0);
  TextColor(7);
  For I:=0 to 27 do Begin
   PutElem(I,J);
   If CurrTab[J,I]='$'Then Inc(Gold);
  End;
  TextBackground(6);
  TextColor(Red);
  Write({$IFDEF FPC}' '{$ELSE}Chr(177){$ENDIF});
  TextBackground(0);
  TextColor(7);
  WriteLn;
 End;
 TextColor(15);
 TextBackground(0);
 GotoXY(29,18);
 Write('Niveau : ',Level);
End;

Procedure HideMan;Begin
 GotoXY(2+XMan,2+YMan);
 PutElem(XMan,YMan);
End;

Procedure HideEnnemi(N:Byte);Begin
 GotoXY(2+XEnnemi[N],2+YEnnemi[N]);
 PutElem(XEnnemi[N],YEnnemi[N]);
End;

Procedure ShowMan;Begin
 GotoXY(2+XMan,2+YMan);
 TextColor(Cyan);
 Write({$IFDEF FPC}'&'{$ELSE}Chr(228){$ENDIF});
 TextColor(7);
End;

Procedure ShowEnnemi(N:Byte);Begin
 GotoXY(2+XEnnemi[N],2+YEnnemi[N]);
 TextColor(White);
 Write({$IFDEF FPC}'0'{$ELSE}Chr(228){$ENDIF});
 TextColor(7);
End;

Procedure LeftMan;Begin
 If XMan>0Then Begin
  If CurrTab[YMan,XMan-1]='#'Then Begin
   XManRun:=0;
   Exit;
  End;
  HideMan;
  XMan:=XMan-1;
  XManRun:=-1;
  If CurrTab[YMan,XMan]='$'Then Begin
   CurrTab[YMan,XMan]:=' ';
   Score:=Score+250;
   Dec(Gold);
   ShowScore;
  End;
  ShowMan;
  If CurrTab[YMan,XMan]<>'-'Then Begin
   If CurrTab[YMan+1,XMan]in[' ','-']Then YManRun:=1;
  End;
 End;
End;

Procedure RightMan;Begin
 If XMan<27Then Begin { Right }
  If CurrTab[YMan,XMan+1]='#'Then Begin
   XManRun:=0;
   Exit;
  End;
  HideMan;
  XMan:=XMan+1;
  XManRun:=1;
  If CurrTab[YMan,XMan]='$'Then Begin
   CurrTab[YMan,XMan]:=' ';
   Score:=Score+250;
   Dec(Gold);
   ShowScore;
  End;
  ShowMan;
  If CurrTab[YMan,XMan]<>'-'Then Begin
   If CurrTab[YMan+1,XMan]in[' ','-']Then YManRun:=1;
  End;
 End;
End;

Function UpMan:Boolean;Begin
 UpMan:=False;
 If YMan>0Then Begin { Up }
  If CurrTab[YMan,XMan]in['H','S']Then Begin
   XManRun:=0;
   HideMan;
   Dec(YMan);
   ShowMan;
   UpMan:=True;
  End;
 End;
End;

Function DownMan:Boolean;Begin
 DownMan:=False;
 If CurrTab[YMan+1,XMan]in['H','S']Then Begin
  XManRun:=0;
  HideMan;
  Inc(YMan);
  ShowMan;
  DownMan:=True;
 End
  Else
 If CurrTab[YMan+1,XMan]in[' ']Then Begin
  XManRun:=0;
  HideMan;
  Inc(YMan);
  ShowMan;
  DownMan:=True;
  YManRun:=1;
 End;
End;

Procedure RunGame;
Var
 I,Skip:Byte;
 K:Char;
Begin
 ShowLife;
 ShowScore;
 Skip:=0;
 Repeat
  Repeat
   Delay(150);
   If YManRun>0Then Begin
    HideMan;
    If CurrTab[YMan,XMan]='-'Then YManRun:=0
                             Else Inc(YMan);
    If CurrTab[YMan+1,XMan]<>' 'Then Begin
     If CurrTab[YMan+1,XMan]='-'Then Inc(YMan);
     YManRun:=0;
    End;
    ShowMan;
   End
    Else
   If XManRun<>0Then Begin
    If XManRun<0Then LeftMan Else
    If XManRun>0Then RightMan;
   End;
   If Skip=0Then For I:=0 to 9 do Begin
    If(XEnnemi[I]or YEnnemi[I])>0Then Begin
     HideEnnemi(I);
     If CurrTab[YEnnemi[I]+1,XEnnemi[I]]in[' ','-']Then Begin
      Inc(YEnnemi[I]);
     End
      Else
     If(YMan>YEnnemi[I])and(CurrTab[YEnnemi[I]+1,XEnnemi[I]]in['H'])Then Inc(YEnnemi[I]) Else
     If(XMan<XEnnemi[I])Then Begin
      If XEnnemi[I]>0Then Begin
       If CurrTab[YEnnemi[I],XEnnemi[I]-1]in[' ','H','S','-','$']Then Dec(XEnnemi[I]);
      End;
     End
      Else
     If XMan>XEnnemi[I]Then Begin
      If CurrTab[YEnnemi[I],XEnnemi[I]+1]in[' ','H','S','-','$']Then Inc(XEnnemi[I]);
     End;
     ShowEnnemi(I);
    End;
    If(XEnnemi[I]=XMan)and(YEnnemi[I]=YMan)Then Begin
     GotoXY(10,10);
     TextColor(LightRed);
     If Life=0Then Begin
      Write('PARTIE TERMINER !');
      Exit;
     End
      Else
     Write('ATTRAPPER !');
     If ReadKey=#0Then ReadKey;
     Dec(Life);
     XManRun:=0;
     YManRun:=0;
     LoadCurrLevel;
     ShowLevel;
     ShowLife;
     Break;
    End;
   End;
   Skip:=(Skip+1)and 3;
  Until Keypressed;
  K:=ReadKey;
  If(WaitYManRun=-1)and(UpMan)Then Begin
   WaitYManRun:=0;
  End
   Else
  If(WaitYManRun=1)and(DownMan)Then Begin
   WaitYManRun:=0;
  End
   Else
  Case K of
   ' ':XManRun:=0;
   #0:Case ReadKey of
    #72:Begin
     If(UpMan)Then Begin
      WaitYManRun:=0;
     End
      Else
     If(Gold<=0)and(YMan=0)Then Begin
      Inc(Level);
      XManRun:=0;
      YManRun:=0;
      LoadCurrLevel;
      ShowLevel;
      ShowLife;
     End
      Else
     WaitYManRun:=-1;
    End;
    #75:LeftMan; { Left }
    #77:RightMan; { Right }
    #80:Begin { Down }
     If(DownMan)Then Begin
      WaitYManRun:=0;
     End
      Else
     WaitYManRun:=1;
    End
   End;
  End;
 Until K=#27;
End;

BEGIN
 InitGame;
 ShowLevel;
 RunGame;
 CursorOn;
END.